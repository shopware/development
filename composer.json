{
    "name": "shopware/development",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "custom/plugins/*",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "custom/plugins/*/packages/*",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "platform",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Shopware\\Development\\": "src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "composer-runtime-api": "^2.0",
        "shopware/platform": "6.4.x@dev || dev-trunk"
    },
    "scripts": {
        "post-update-cmd": [
            "[ ! -d platform/.git ] || [ -L vendor/shopware/platform ] || (echo 'vendor/shopware/platform should be a symlink to platform'; exit 1)",
            "[ ! -d vendor/shopware/platform/.git ] || ln -sf $PWD/dev-ops/pre-commit vendor/shopware/platform/.git/hooks/pre-commit"
        ]
    },
    "require-dev": {
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "maltyxx/images-generator": "1.0.0",
        "brianium/paratest": "^6.2",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "fakerphp/faker": "1.14.1",
        "johnkary/phpunit-speedtrap": "3.3.0",
        "league/flysystem-memory": "1.0.2",
        "mbezhanov/faker-provider-collection": "2.0.1",
        "nikic/php-parser": "4.10.4",
        "opis/json-schema": "1.0.19",
        "phpunit/php-code-coverage": "9.2.5",
        "phpunit/phpunit": "9.5.2",
        "smalot/pdfparser": "0.14.0",
        "symfony/browser-kit": "~4.4.4",
        "symfony/dependency-injection": "~4.4.4 || ~5.2.3",
        "symfony/phpunit-bridge": "~4.4.4 || ~5.2.3",
        "symfony/stopwatch": "~4.4.4 || ~5.2.3",
        "symfony/var-dumper": "~4.4.4 || ~5.2.3",
        "symfony/web-profiler-bundle": "~4.4.4 || ~5.2.3",
        "symfony/dom-crawler": "~4.4.4 || ~5.2.3"
    }
}
